<templateSet group="AndroidLog">
  <template name="dtag" value="private static final String TAG = &quot;$CLASS_NAME$&quot;;" description="Define tag" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
</templateSet>